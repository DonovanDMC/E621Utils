{
  "name": "e621-tags",
  "version": "1.0.0",
  "description": "A tag checker for e621.",
  "main": "index.js",
  "scripts": {
    "up:d": "sudo docker compose pull && sudo docker compose up -d",
    "up": "sudo docker compose pull && sudo docker compose up",
    "down": "sudo docker compose down",
    "up:dev": "sudo docker compose up",
    "dev": "npm run down && npm run up:dev",
    "dev:build": "npm run down && npm run up",
    "update": "git pull && git submodule update --init --recursive --remote && docker compose pull && npm run down&& npm run up:d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DonovanDMC/E621Tags.git"
  },
  "author": "Donovan Daniels <hewwo@yiff.rocks>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DonovanDMC/E621Tags/issues"
  },
  "homepage": "https://github.com/DonovanDMC/E621Tags#readme",
  "type": "module",
  "devDependencies": {
    "@swc/core": "^1.3.19",
    "@swc/helpers": "^0.4.14",
    "@types/chunk": "^0.0.0",
    "@types/node": "^18.11.9",
    "@uwu-codes/eslint-config": "^1.1.6",
    "@uwu-codes/tsconfig": "^1.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "chunk": "^0.0.3",
    "e621": "^2.2.6",
    "eris": "^0.17.1",
    "tsafe": "^1.2.1"
  },
  "private": true
}
